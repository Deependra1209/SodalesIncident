COLUMN TABLE INC_T_RTWTC (
  RTCID INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START 
WITH 1 INCREMENT BY 1),
  INCID INTEGER COMMENT 'Incident Id',
  RTWID INTEGER COMMENT 'RTW Unique Id',
  EMPID NVARCHAR(30) COMMENT 'Send To/Cc Id',
  TCFLG INTEGER COMMENT 'Send To/Cc Identification Flag',
  ISDEL NVARCHAR(1) COMMENT 'Is delete',
  CRTBY NVARCHAR(100) COMMENT 'Created Date',
  CRTDT DATE COMMENT 'Created Date',
  CRTTM TIME COMMENT 'Created Time'
);