COLUMN TABLE INC_T_MOBTR (
  MTRID INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START 
WITH 1 INCREMENT BY 1) COMMENT 'Unique Key',
  INCID INTEGER COMMENT 'Incident Id',
  TRACK INTEGER COMMENT 'Track/Untrack Flag',
  ISDEL NVARCHAR(1) COMMENT 'Is delete',
  CRTBY NVARCHAR(100) COMMENT 'Created By',
  CRTDT DATE COMMENT 'Created Date',
  CRTTM TIME COMMENT 'Created Time'
);