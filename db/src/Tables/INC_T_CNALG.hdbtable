COLUMN TABLE INC_T_CNALG (
  CALID INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START 
WITH 1 INCREMENT BY 1) COMMENT 'Config Audit Log Unique Id',
  APPNM NVARCHAR(50) COMMENT 'Application Name',
  SRVNM NVARCHAR(50) COMMENT 'Function Name',
  PLOAD CLOB MEMORY THRESHOLD 1000 COMMENT 'Payload',
  ISDEL NVARCHAR(1) COMMENT 'Deletable Flag',
  CRTBY NVARCHAR(100) COMMENT 'Created By',
  CRTDT DATE COMMENT 'Created Date',
  CRTTM TIME COMMENT 'Created Time'
);