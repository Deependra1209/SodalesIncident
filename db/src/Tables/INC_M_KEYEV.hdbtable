COLUMN TABLE INC_M_KEYEV (
  KYEID INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START 
WITH 1 INCREMENT BY 1) COMMENT 'Unique Id',
  KEYID INTEGER COMMENT 'Key Identifier Id',
  EVNID INTEGER COMMENT 'Event Id',
  CRTBY NVARCHAR(100) COMMENT 'Created By',
  CRTDT DATE COMMENT 'Created Date',
  CRTTM TIME COMMENT 'Created Time',
  ISDEL NVARCHAR(1)
);