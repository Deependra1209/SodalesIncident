COLUMN TABLE INC_M_ROLPH (
  RLPID INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START 
WITH 1 INCREMENT BY 1) COMMENT 'Unique Id',
  ROLNM NVARCHAR(100) COMMENT 'Role Name',
  UIROL NVARCHAR(100) COMMENT 'Launchpad Role',
  APPNM NVARCHAR(100) COMMENT 'Application Name',
  ROLPH NVARCHAR(250) COMMENT 'Role path',
  RGUID NVARCHAR(20) COMMENT 'GUID',
  REBHF INTEGER COMMENT 'Reporting on behalf flag',
  ISDEL NVARCHAR(1) COMMENT 'Is deleted',
  CRTBY NVARCHAR(100) COMMENT 'Created By',
  CRTDT DATE COMMENT 'Created Date',
  CRTTM TIME COMMENT 'Created Time'
);